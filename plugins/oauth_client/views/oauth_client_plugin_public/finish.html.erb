<script>
  window.addEventListener("message", function(ev) {
    if (ev.data.message === "requestOauthClientPluginResult") {
      ev.source.postMessage({ message: "oauthClientPluginResult", logged_in: <%= logged_in? %>, private_token: '<%= private_token %>', user: <%= current_user.person.to_json %> }, "*");
    }
  });
</script>
